    <PackageVersion Include="JetBrains.Annotations" Version="2025.2.0" />
    <PackageVersion Include="Microsoft.Net.Compilers.Toolset" Version="5.0.0-2.25406.102" />
    <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.13.0" />
    <PackageVersion Include="Microsoft.SourceLink.GitHub" Version="8.0.0" />
    <PackageVersion Include="Polyfill" Version="9.0.0-beta.15" />
    <PackageVersion Include="PolySharp" Version="1.15.0" />
    <PackageVersion Include="NUnit" Version="4.4.0" />
    <!--<PackageVersion Include="NUnit" Version="3.14.0" />-->
    <PackageVersion Include="NUnit.Analyzers" Version="4.10.0" />
    <PackageVersion Include="NUnit3TestAdapter" Version="5.1.0" />
    <PackageVersion Include="ThisAssembly.Git" Version="2.0.14" />
  </ItemGroup>
