  <!-- DOC: Central Package Management -->
  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    <!-- DOC: Modify file's hash when VersionOverride calculation changes -->
    <TargetTextJsonVersionFallback>9</TargetTextJsonVersionFallback>
